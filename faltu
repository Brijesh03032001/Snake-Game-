#include<stdio.h>
	int flag=0;
void find(int x,int y)
{

	if(x%10==y){
		flag=1;
		return ;
		
	}
	if(x<10){return;}
	x=x/10;
	find(x,y);
	
	
	}
	

int main()
{
	int num,digit;
	
	printf("Enter the number(x):\n");
	scanf("%d",&num);
	printf("\n\n\nEnter the digit(y):\n");
	scanf("%d",&digit);
	if(num>=1 && (0<=digit<10))
{
				find(num,digit);
		if(flag==1)
		{
			printf("\n\nYes");
		}
		if(flag==0)
		{
			printf("\n\nNo");
		}
}
}
